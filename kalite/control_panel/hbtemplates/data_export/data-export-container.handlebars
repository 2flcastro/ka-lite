    <div class="form-group">
        <label for="resource-name">Resource</label>
        <select class="form-control" id="resource-id">
            <option data-resource-id="facility_user">Facility User Logs</option>
            <option data-resource-id="test_log">Test Logs</option>
            <option data-resource-id="attempt_log">Attempt Logs</option>
        </select>
    </div>
